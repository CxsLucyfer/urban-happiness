{
  "bracketSpacing": true,
  "overrides": [{
    "files": ["*.html"],
    "options": {
      "htmlWhitespaceSensitivity": "ignore",
      "jsxBracketSameLine": true
    }
  }],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false
}